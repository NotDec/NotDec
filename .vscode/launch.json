{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: notdec_retypd_provider",
      "type": "python",
      "request": "launch",
      "module": "notdec_retypd_provider",
      "cwd": "${workspaceFolder}/src/optimizers",
      "justMyCode": false,
      "args": [
        "--json-in",
        "${file}",
        "--debug-dir",
        "${fileDirname}"
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Structural emcc_print",
      // "program": "${workspaceFolder}/build/bin/notdec",
      "program": "${command:cmake.launchTargetPath}",
      "args": [
        "${workspaceFolder}/test/wasm/emcc-print.wat",
        "-o",
        "${workspaceFolder}/test/wasm/emcc-print.wat.c",
        "--log-level=debug",
        "--no-mem-initializer",
        "--disable-all-pass"
      ],
      "cwd": "${workspaceFolder}",
      // "preLaunchTask": "build"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decompile emcc_print",
      // "program": "${workspaceFolder}/build/bin/notdec",
      "program": "${command:cmake.launchTargetPath}",
      "args": [
        "${workspaceFolder}/test/wasm/emcc-print.wat",
        "-o",
        "${workspaceFolder}/test/wasm/emcc-print.wat.ll",
        "--log-level=debug",
        "--split-mem",
      ],
      "cwd": "${workspaceFolder}",
      // "preLaunchTask": "build"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decompile",
      // "program": "${workspaceFolder}/build/bin/notdec",
      "program": "${command:cmake.launchTargetPath}",
      "args": [
        "${file}",
        "-o",
        "${file}.ll",
        "-debug-only=retypd",
        // "--print-before=linear-allocation-recovery"
      ],
      "cwd": "${workspaceFolder}",
      // "preLaunchTask": "build"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "recompile",
      "program": "${command:cmake.launchTargetPath}",
      "args": [
        "${file}",
        "-o",
        "${file}.ll",
        "--log-level",
        "debug",
        "--compat-mode",
        "--recompile",
        "--expand-mem=false"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "GraphTest SaturationPaperTest",
      "program": "${workspaceFolder}/build/bin/GraphTest",
      "args": [
        "--gtest_filter=Retypd.SaturationPaperTest",
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "CMake: build",
      "env": {
        "DEBUG_TRANS_INIT_GRAPH": "1",
        "DEBUG_TRANS_SAT_GRAPH": "1"
      }
    }
  ]
}
